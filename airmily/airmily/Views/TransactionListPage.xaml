             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             xmlns:controls="clr-namespace:airmily.Views.Controls;assembly=airmily"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="airmily.Views.TransactionListPage">

	<StackLayout>
		<Frame>
			
		</Frame>
		<Frame>
			<controls:ValuedListView ItemsSource="{Binding TransactionList}" RowHeight="40" HasUnevenRows="True" BackgroundColor="#B9B9B9"/>
		</Frame>
	</StackLayout>
	
</ContentPage>